        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "args": ["local", "-m", "8b", "./fixtures/example.min.js"]
      },
      {
        "type": "node",
        "request": "launch",
        "name": "反编译async.js",
        "runtimeExecutable": "node",
        "runtimeArgs": ["--import", "tsx"],
        "program": "${workspaceFolder}/src/index.ts",
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
